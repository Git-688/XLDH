    // è·å–ä¸€çº§åˆ†ç±»è¯¦æƒ…
    getCategoryInfo: function(category) {
        return {
            name: category,
            subCategories: Object.keys(this.categories[category] || {}),
            description: this.descriptions?.[category] || '',
            icon: this.categoryIcons?.[category] || 'fas fa-folder'
        };
    },
    
    // è·å–äºŒçº§åˆ†ç±»æ•°æ®
    getLevel2Data: function(level2) {
        for (const category in this.categories) {
            for (const subCategory in this.categories[category]) {
                if (subCategory === level2) {
                    return this.categories[category][subCategory];
                }
            }
        }
        return [];
    },
    
    // è·å–æ‰€æœ‰ç½‘ç«™æ•°æ®
    getAllWebsites: function() {
        const allWebsites = [];
        for (const category in this.categories) {
            for (const subCategory in this.categories[category]) {
                const websites = this.categories[category][subCategory];
                websites.forEach(website => {
                    website.category = category;
                    website.subCategory = subCategory;
                    allWebsites.push(website);
                });
            }
        }
        return allWebsites;
    },
    
    // æœç´¢ç½‘ç«™
    searchWebsites: function(keyword) {
        const results = [];
        const allWebsites = this.getAllWebsites();
        
        keyword = keyword.toLowerCase().trim();
        if (!keyword) return [];
        
        for (const website of allWebsites) {
            if (
                website.title.toLowerCase().includes(keyword) ||
                website.description.toLowerCase().includes(keyword) ||
                (website.category && website.category.toLowerCase().includes(keyword)) ||
                (website.subCategory && website.subCategory.toLowerCase().includes(keyword))
            ) {
                results.push(website);
            }
        }
        
        return results.slice(0, 50); // é™åˆ¶ç»“æœæ•°é‡
    },
    
